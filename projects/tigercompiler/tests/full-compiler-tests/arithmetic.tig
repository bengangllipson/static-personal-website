let
    function printint(i: int) =
      let function f(i:int) = if i>0 
    	     then (f(i/10); print(chr(i-i/10*10+ord("0"))))
       in if i<0 then (print("-"); f(-i))
          else if i>0 then f(i)
          else print("0")
      end
    in
(
    printint(1); print("\n");
    printint(1+2); print("\n");
    printint(1+2*9); print("\n");
    printint(1&1); print("\n");
    printint(0&1); print("\n");
    printint(0|1); print("\n");
    printint(0|0); print("\n");
    printint(1 + 4/5); print("\n");
    printint(1 + 99/11); print("\n");
    printint((7-2)*(7+2))
)
end